def web_brouser()
   selected=strvar.get()
   if selected=="Stambul"
     webbrouser.open_new("https://traveller-eu.ru/stambul")
def web_brouser()
   selected=strvar.get()
   if selected=="London"
     webbrouser.open_new("https://www.momondo.dk/flybilletter/london?sck=SEM&skipapp=true")
def web_brouser()
   selected=strvar.get()
   if selected=="Viden"
     webbrouser.open_new("https://ordnet.dk/ddo/ordbog?query=viden")
def web_brouser()
   selected=strvar.get()
   if selected=="Ottaava"
     webbrouser.open_new("https://ottawa.ca/en")
but=tk.But(wind, text="please click me", bg="LightGreen", command=web_brouser)
but.pack
