import tkinter as tk
wind=tk.Tk()
wind.title("My optionmenu")
wind.geometry("450x300+350+30")
wind.resizable(0, 0)
wind.config(bg="orange")
my_cities=["Lviv", "Odessa", "Tokio", "Stambul", "London", "Viden", "Ottaava"]
strvar=tk.StringVar(wind)
strvar.set(my_cities[2])
opm=tk.OptionMenu(wind,strvar,*my_cities
opm.pack(pady=8)

def web_brouser()
   selected=strvar.get()
   if selected=="Lviv"
     webbrouser.open_new("https://lviv.travel/en")
